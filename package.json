    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cors-anywhere": "^0.4.1",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.19.1",
    "mongoose": "^5.7.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "validator": "^13.7.0"
