{
  "name": "ledger-backend",
  "version": "1.0.0",
  "description": "Backend for ledger on the go app",
  "main": "server.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "client-install": "npm install --prefix Ledger-on-go",
    "start": "node server.js",
